# programa con funcion
programa seis:
	tipos
		tvector = vector [1..5] de entero;
	variables 
		c, d,suma: entero;
		h : tvector;
	subprogramas
		funcion sumar (a,b : entero): entero:
			variables suma: entero;
		comienzo
			suma = a + b;

			devolver suma;
		fin;


comienzo
	c = 5;
	d = 18;
	suma = sumar(h[1],d);

	escribir(suma);

fin.
